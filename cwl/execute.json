{
"$schema": "http://json-schema.org/schema#",
  "description": "Schema defining an execution command to execute models",
  "type": "object",
  "required": [ 
    "job",
    "clusterSpec",
    "inputs"
  ],
 
  "properties": {
 
    "job": {
      "type": "string",
      "command": "job-command"
    },
    
    "clusterSpec": {
      "type": "string",
      "clusterName": "Name",
      "someField": "some-value"
    },
    "inputs": {
      "type": "object"
    }
 }
}
